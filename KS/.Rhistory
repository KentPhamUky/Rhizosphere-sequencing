# Ordinate
Clean_pcoa <- ordinate(
physeq = Clean,
method = "PCoA",
distance = "bray"
)
# Plot
plot_ordination(
physeq = Clean,
ordination = Clean_pcoa,
color = "Timtest",
shape = "Year",
title = "PCoA of Tim Test"
) +
scale_color_manual(values = c("#a65628", "red", "#ffae19",
"#4daf4a", "#1919ff", "darkorchid3", "magenta")
) +
geom_point(aes(color = Timtest), alpha = 0.7, size = 4) +
geom_point(colour = "grey90", size = 1.5)
